{
  "name": "auth-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "yarn prisma:generate && jest",
    "test:watch": "yarn prisma:generate && jest --watch",
    "test:coverage": "yarn prisma:generate && jest --coverage",
    "prisma:generate": "prisma generate --schema ./db/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema ./db/schema.prisma",
    "seed": "prisma db seed --schema ./db/schema.prisma"
  },
  "dependencies": {
    "@auth/pg-adapter": "^1.0.1",
    "@auth/prisma-adapter": "^1.0.0",
    "@google-cloud/local-auth": "^2.1.0",
    "@google-cloud/storage": "^7.0.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.3",
    "dropbox": "^10.34.0",
    "googleapis": "^105.0.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^10.2.2",
    "next": "14.2.3",
    "next-auth": "^4.24.7",
    "nodemailer": "^6.10.1",
    "pg": "^8.11.5",
    "prisma": "^5.13.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
