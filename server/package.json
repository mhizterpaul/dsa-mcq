    "@types/react": "18.3.2",
    "@types/react-dom": "18.3.0",
    "@types/uuid": "^8.3.4",
    "csv-parse": "^5.5.5",
    "jest": "^29.7.0",
    "next-test-api-route-handler": "^3.1.8",
    "prisma-import": "^1.0.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "5.4.5"
  }
